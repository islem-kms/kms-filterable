{
  "name": "islem-kms/filterable",
  "description": "An Eloquent way to filter Eloquent Models",
  "keywords": [
    "laravel",
    "eloquent",
    "model",
    "search",
    "query",
    "filter"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Islem Khemissi",
      "email": "khemissi.islem@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/console": "~5.0",
    "illuminate/filesystem": "~5.0",
    "illuminate/database": "~5.0",
    "illuminate/support": "~5.0",
    "illuminate/config": "~5.0",
    "illuminate/pagination": "~5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.4.0",
    "mockery/mockery": "^0.9.5"
  },
  "autoload": {
    "psr-4": {
      "IslemKms\\EloquentFilter\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "IslemKms\\EloquentFilter\\ServiceProvider"
      ]
    }
  }
}
